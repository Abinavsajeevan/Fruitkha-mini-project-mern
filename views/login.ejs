   <!--================login_part Area =================-->
<section class="login_part section_padding" style="background:url('./assets/img/hero-bg-2.jpg') center/cover no-repeat; padding:80px 0;">
    <div class="container ">
        <div class="row justify-content-center align-items-center ">
            
            <!-- Left Column: New User -->
            <div class="col-lg-6 col-md-6 mb-4 mb-md-0">
                <div class="login_part_text text-center p-5" style="background: linear-gradient(135deg, #f0f0f0a3, #fba661af); color: white; border-radius: 10px;">
                    <h2 style="color: #051922;">New to our Shop?</h2>
                    <p class="fs-5">There are fresh and delicious fruits are available in our shop. Lorem, ipsum dolor sit amet consectetur adipisicing elit. Voluptatem illo, maiores excepturi laborum facere quidem provide</p>
                    <a href="signup.html" class="btn btn-outline-light mt-3">Create an Account</a>
                </div>
            </div>

            <!-- Right Column: Login Form -->
            <div class="col-lg-6 col-md-6" >
                <div class="login_part_form p-5" style=" border-radius: 10px; margin-left: 23px;">
                    <h3 class="mb-4">Welcome Back! <br> Please Sign in now</h3>

                    <!-- Google Login Button -->
                    <button class="btn btn-outline-danger w-100 mb-3">
                        <i class="fab fa-google me-2"></i> Login with Google
                    </button>
                    <div class="text-center text-muted mb-3">or use your email</div>

                    <!-- Login Form -->
                    <form>
                        <div class="mb-3">
                            <input type="text" class="form-control" placeholder="Username" required>
                        </div>
                        <div class="mb-3">
                            <input type="password" class="form-control" placeholder="Password" required>
                        </div>
                        <div class="d-flex justify-content-between align-items-center mb-3">
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" value="" id="rememberMe">
                                <label class="form-check-label" for="rememberMe">Remember me</label>
                            </div>
                            <a href="#">Forget Password?</a>
                        </div>
                        <button type="submit" class="btn btn-primary w-100">Log In</button>
                    </form>
                </div>
            </div>

        </div>
    </div>
</section>

