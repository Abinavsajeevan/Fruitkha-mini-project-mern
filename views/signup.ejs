<!--================ Register Section =================-->
<section class="register_part section_padding position-relative" 
  style="background:url('./assets/img/hero-bg-2.jpg') center/cover no-repeat; padding:80px 0;">
  <div class="container">
    <div class="row justify-content-end">
      <!-- Text on left -->
      <div class="col-lg-4 d-none d-lg-flex flex-column justify-content-center">
        <h1 class="fw-bold mb-3" style="color:#F28123; font-size:2.2rem;">
          Join Fruitkha Today!
        </h1>
        <h6 class="text-dark fs-5">
          Get access to exclusive deals, track your orders, and enjoy a personalized shopping experience.
        </h6>
      </div>

      <!-- Form on right -->
      <div class="col-lg-6 col-md-8">
        <div class="p-5 rounded-4 shadow-lg" 
          style="background:rgba(255, 255, 255, 0); backdrop-filter:blur(8px);">
          <h3 class="text-center fw-bold mb-4" style="color:#F28123;">Create Your Account</h3>

          <!-- Google button -->
          <button class="btn w-100 mb-4 border-0 text-white" 
            style="background-color:#db4437; border-radius:50px;">
            <i class="fab fa-google me-2"></i> Sign Up with Google
          </button>

          <div class="text-center text-light mb-4">or use your email</div>

          <form action="#">
            <div class="form-floating mb-3">
              <input type="text" class="form-control rounded-pill bg-transparent text-white border-light" id="name" placeholder="Full Name">
              <label for="name" class="text-light">Full Name</label>
            </div>
            <div class="form-floating mb-3">
              <input type="email" class="form-control rounded-pill bg-transparent text-white border-light" id="email" placeholder="Email">
              <label for="email" class="text-light">Email Address</label>
            </div>
            <div class="form-floating mb-3">
              <input type="password" class="form-control rounded-pill bg-transparent text-white border-light" id="password" placeholder="Password">
              <label for="password" class="text-light">Password</label>
            </div>
            <div class="form-floating mb-3">
              <input type="password" class="form-control rounded-pill bg-transparent text-white border-light" id="confirm" placeholder="Confirm Password">
              <label for="confirm" class="text-light">Confirm Password</label>
            </div>

            <div class="form-check mb-4">
              <input class="form-check-input" type="checkbox" id="terms">
              <label class="form-check-label text-light" for="terms">
                I agree to the <a href="#" style="color:#F28123;">Terms & Conditions</a>
              </label>
            </div>

            <button type="submit" class="btn w-100 fw-bold text-white" 
              style="background-color:#F28123; border-radius:50px;">
              Register
            </button>
          </form>

          <div class="mt-4 text-center text-light">
            Already have an account? 
            <a href="login.html" style="color:#F28123;">Log In</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

